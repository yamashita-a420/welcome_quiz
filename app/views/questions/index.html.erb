<% content_for(:title, t('.title')) %>
<div class="common_container">
  <div class="flex flex-col title-font font-medium items-center text-gray-900 mb-2 md: mb-0">
    <h1 class="ml-3 text-3xl m-3"><%= t '.title' %></h1>

    <% if @questions.present? %>
      <%= render @questions %>
    <% else %>
      <p><%= t('.no_result') %></p>
    <% end %>
  </div>
</div>
